<%= form_for [:admin, @train, @ticket] do |f| %>

  <%= render 'common/errors', resource: @ticket %>

  <div class="field">
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </div>

  <div class="field">
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
  </div>

  <div class="field">
    <%= f.label :middle_name %><br>
    <%= f.text_field :middle_name %>
  </div>

  <div class="field">
    <%= f.label :passport_series %><br>
    <%= f.text_field :passport_series %>
  </div>

  <div class="field">
    <%= f.label :passport_number %><br>
    <%= f.text_field :passport_number %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>
