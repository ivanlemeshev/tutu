<tr>
  <td><%= coupe_car.type_name %></td>
  <td><%= coupe_car.bottom_seats %></td>
  <td><%= coupe_car.top_seats %></td>
  <td></td>
  <td></td>
  <td>
    <% if coupe_car.train.present? %>
      <%= coupe_car.train.number %>
    <% else %>
      <%= t('common.not_set') %>
    <% end %>
  </td>
  <td><%= link_to t('common.show'), admin_car_path(coupe_car) %></td>
  <td><%= link_to t('common.edit'), edit_admin_car_path(coupe_car) %></td>
  <td><%= link_to t('common.destroy'),
                  admin_car_path(coupe_car),
                  method: :delete,
                  data: { confirm: t('common.sure') } %></td>
</tr>
