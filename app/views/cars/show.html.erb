<p id="notice"><%= notice %></p>

<p>
  <strong>Type:</strong>
  <%= @car.type_name %>
</p>

<p>
  <strong>Down place count:</strong>
  <%= @car.bottom_seats %>
</p>

<p>
  <strong>Up place count:</strong>
  <%= @car.top_seats %>
</p>

<p>
  <strong>Train:</strong>
  <% if @car.train.present? %>
    <%= @car.train.number %>
  <% else %>
    not set
  <% end%>
</p>

<%= link_to 'Edit', edit_car_path(@car) %> |
<%= link_to 'Back', cars_path %>
